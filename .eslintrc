{
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"useJSXTextNode": true,
		"sourceType": "module"
	},
	"plugins": ["import", "promise", "jsx-a11y", "compat", "prettier", "react", "@typescript-eslint"],
	"env": {
		"browser": true,
		"node": true
	}
}
